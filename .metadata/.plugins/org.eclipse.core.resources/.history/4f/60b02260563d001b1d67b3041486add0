package stepdef;


import cucumber.api.PendingException;
import io.cucumber.java.en.But;
import io.cucumber.java.en.Given;
import io.cucumber.java.en.Then;
import io.cucumber.java.en.When;

public class AmazonSearchSteps {
	

	   
	    @Given("^User should open Chrome browser$")
	    public void user_should_open_chrome_browser() throws Throwable {
	        throw new PendingException();
	    }

	    @Given("^The user is already on the Amazon Home Page$")
	    public void the_user_is_already_on_the_amazon_home_page() throws Throwable {
	        throw new PendingException();
	    }

	    @When("^user Click on search bar and type \"([^\"]*)\"$")
	    public void user_click_on_search_bar_and_type_something(String productname, String strArg1) throws Throwable {
	        throw new PendingException();
	    }

	    @Then("^user should be shown all the availble \"([^\"]*)\" in the inventory$")
	    public void user_should_be_shown_all_the_availble_something_in_the_inventory(String productname, String strArg1) throws Throwable {
	        throw new PendingException();
	    }

	    @But("^User should not see the \"([^\"]*)\"$")
	    public void user_should_not_see_the_something(String excludedproduct, String strArg1) throws Throwable {
	        throw new PendingException();
	    }
	}

